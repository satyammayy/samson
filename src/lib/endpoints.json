{
    "Note":"Never Share this URL and the SESSION_ID with anyone! Make sure to include the \"session\" query with every request",
    "client":{
        "baseurl": "/client",
        "endpoints":[
            {
                "endpoint":"/",
                "method":"GET",
                "query":null,
                "description":"Main endpoint"
            },
            {
                "endpoint":"/qr",
                "method":"GET",
                "query":[
                    "session"
                ],
                "description":"Displays the Auth QR"
            },
            {
                "endpoint":"/config",
                "method":"GET",
                "query":[
                    "session"
                ],
                "description":"Main endpoint"
            },
            {
                "endpoint":"/wa",
                "method":"GET",
                "query":[
                    "state"
                ],
                "description":"turn on or off the client"
            },
            {
                "endpoint":"user",
                "method":"GET",
                "query":[
                    "session",
                    "jid"
                ],
                "description":"retrieves the given user's info"
            },
            {
                "endpoint":"/pfp",
                "method":"GET",
                "query":[
                    "session",
                    "jid"
                ],
                "description":"Retrieves the pfp of the given JID"
            },
            {
                "endpoint":"/client",
                "method":"GET",
                "query":[
                    "state"
                ],
                "description":"turn on or off the client"
            }
        ]
    }
}